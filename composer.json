{
  "name": "annotate/packages",
  "type": "library",
  "description": "Package management extension for Annotate Framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Michal Vyšinský",
      "email": "michal@vysinsky.cz"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:AnnotateFramework/coding-standard.git"
    }
  ],
  "require": {
    "php": ">=5.6",
    "kdyby/events": "~3.0.0",
    "nette/bootstrap": "~2.4.0",
    "annotate/framework": "@dev",
    "annotate/diagnostics": "@dev"
  },
  "require-dev": {
    "annotate/templating": "@dev",
    "annotate/themes": "@dev",
    "nette/tester": "@dev",
    "latte/latte": "^2.4",
    "janmarek/mockista": "@dev",
    "squizlabs/php_codesniffer": "~2.1",
    "dotblue/codesniffer-ruleset": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Annotate\\Packages\\": "src/"
    }
  }
}
